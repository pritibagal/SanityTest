<?xml version="1.0" encoding="UTF-8"?>

<extentreports>

    <configuration>

        <!-- report theme --> <!-- standard, dark -->

        <theme>light</theme>


        <!-- document encoding -->  <!-- defaults to UTF-8 -->


        <encoding>UTF-8</encoding>


        <!-- protocol for script and stylesheets -->   <!-- defaults to https -->

        <protocol>https</protocol>
        <!-- title of the document -->


        <documentTitle>Selenium-TestNG Framework (Extent Report)</documentTitle>


        <!-- report name - displayed at top-nav -->

        <reportName>IntelliDoc Automation Execution Report</reportName>
        <!-- custom javascript  -->
        <scripts>
            <![CDATA[
$(document).ready(function() {
$('#enableDashboard').click();
});
]]>
        </scripts>
        <!-- custom styles  -->
        <styles>
            <![CDATA[
]]>
        </styles>



    </configuration>
</extentreports>